{
  "name": "@wavegrid/backend",
  "version": "1.0.0",
  "scripts": {
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly --poll src/server",
    "start": "ts-node -T --log-error src/server",
    "codegen": "graphql-codegen --config codegen.yml",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nexus/schema": "^0.16.0",
    "@paypal/checkout-server-sdk": "^1.0.2",
    "@prisma/client": "2.7.0",
    "@sendgrid/mail": "^7.2.6",
    "@sentry/node": "^5.24.2",
    "@sentry/tracing": "^5.24.2",
    "aws-sdk": "^2.743.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "discord-oauth2": "^2.5.0",
    "dotenv-flow": "^3.2.0",
    "faker": "^5.1.0",
    "graphql": "14.7.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "4.0.1",
    "graphql-shield": "7.3.6",
    "graphql-yoga": "1.18.3",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.0",
    "multer": "^1.4.2",
    "nexus-plugin-prisma": "0.20.0",
    "sanitize-html": "^2.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@prisma/cli": "2.7.0",
    "@sentry/typescript": "^5.20.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/cuid": "^1.3.1",
    "@types/dotenv-flow": "^3.1.0",
    "@types/faker": "^4.1.12",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jsonwebtoken": "8.5.0",
    "@types/lodash": "^4.14.161",
    "@types/multer": "^1.4.4",
    "@types/node": "12.12.54",
    "@types/sanitize-html": "^1.27.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "^4.0.3"
  }
}
